




stepzen import \
        curl -X POST "https://accounts.spotify.com/api/token" \
        --header "Content-Type: application/x-www-form-urlencoded" \
        --data "grant_type=client_credentials&client_id=fd5fb98a60dc40b1b52f50503e79aadf&client_secret=40e68be0860549499ee4bd79ccfdcf62" \
        --query-name "getToken" \
        --query-type "TokenResponse" \
        --name "auth"



        stepzen import \
        curl "https://api.spotify.com/v1/tracks/11dFghVXANMlKmJXsNCbNl" \
        --header "Authorization: Bearer BQAAyV0uxE2f7aJp392wK4uqQzc4BBNbUR6qwGMk5_vY0H5CTUhXl-NXjDCi-s31Nniuqh5n4joYDRJaC04VIf76HHZJ6i7eFKyUagDC0Twj2z-Coeg" \
        --path-params "/v1/tracks/\$trackId/"  \
        --query-name "getTrack" \
        --query-type "TrackResponse" \
        --name "track" \
        --prefix "Track"




